
<div class="container-fluid">
    <div class="row animate-box">
        <div class="col-md-12 heading text-center"><h2><%= title %></h2></div>
    </div>
    <div class="row">
        <% list_post.forEach(function(post) {%>
        <div class="col-lg-3 col-md-6 col-sm-6 animate-box">
            <a class="fh5co-card" href="/blog/<%= post.id %>.html">
                <img src="<%= post.img %>" alt="Free HTML5 Bootstrap template" class="img-responsive" style="width: 100%;height: 150px;">
                <div class="fh5co-card-body">
                    <h3><%= post.name %></h3>
                    <p><%= post.content %>.</p>
                </div>
            </a>
        </div>
        <% }) %>
    </div>
</div>